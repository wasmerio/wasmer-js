{
  "name": "@wasmer/wasix",
  "version": "0.0.1-alpha.1",
  "main": "dist/lib.min.cjs",
  "module": "dist/lib.min.mjs",
  "unpkg": "dist/lib.umd.min.js",
  "types": "dist/lib.d.ts",
  "keywords": [
    "webassembly",
    "wasm",
    "wasi",
    "wasix"
  ],
  "engines": {
    "node": ">=18"
  },
  "description": "Isomorphic Javascript library for interacting with WASI(X) Modules.",
  "author": "Wasmer Engineering Team <engineering@wasmer.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wasmerio/wasmer-js/issues"
  },
  "homepage": "https://github.com/wasmerio/wasmer-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wasmerio/wasmer-js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "wasm-pack build --release --target web && wasm-strip pkg/wasmer_wasix_js_bg.wasm && rollup -c --environment BUILD:production",
    "dev": "rollup -c -w",
    "lint": "",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js -c jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "clean": "rimraf dist coverage",
    "prepare": "npm-run-all clean lint build test"
  },
  "dependencies": {
    "node-fetch": "^3.3.1",
    "whatwg-worker": "github:jimmywarting/whatwg-worker#master"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "~0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "ts-loader": "^9.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "browserslist": "defaults"
}
